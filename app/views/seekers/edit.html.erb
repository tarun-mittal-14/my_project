


<%= form_with model: @seeker do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>
    <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </div>
    <div>
    <%= form.label :age %><br>
    <%= form.text_field :age %>

  </div>

    <div>
    <%= form.label :qualification %><br>
    <%= form.text_field :qualification %>

  </div>

  <div>
    <%= form.label :experience %><br>
    <%= form.text_field :experince %>

  </div>

   <div>
    <%= form.submit %>
  </div>

<li><%= link_to "back", seeker_path(@seeker) %></li>
<% end %>




















































